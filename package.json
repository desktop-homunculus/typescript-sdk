{
  "name": "@homunculus/sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for building mods and extensions for Desktop Homunculus",
  "author": "notelm",
  "license": "ISC",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [],
  "scripts": {
    "dev": "rollup -c --configPlugin typescript --watch",
    "build": "rollup -c --configPlugin typescript",
    "prepare": "npm run build",
    "check-types": "tsc --noEmit",
    "tsc": "rimraf dist && tsc --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/desktop-homunculus/typescript-sdk.git",
    "directory": "sdk/typescript-sdk"
  },
  "files": [
    "dist"
  ],
  "packageManager": "npm@11.8.0",
  "dependencies": {
    "eventsource": "^4.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.19.33",
    "rimraf": "^6.1.2",
    "rollup": "^4.57.1",
    "rollup-plugin-dts": "^6.3.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  }
}
